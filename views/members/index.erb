<p><b>Current Members</b></p>

<br>

<a href='/members/new'>Add Member</a>

<% @members.each do |member| %>
<p>First Name</p>
<%= member.first_name %>
<p>Last Name</p>
<%= member.last_name %>
<p>Membership: <%= member.membership %></p>
<p><a href="/members/<%= member.id %>/edit">Update</a></p>

  <p><a href="/members/<%= member.id %>/show">Show</a></p>

  <form action="/members/<%= member.id %>/delete" method="POST">
    <input type="submit" value="delete" />
  </form>

  <% end %>
  <hr/>
